with "opengl.gpr";
with "opengl_shared.gpr";
with "FreeTypeAda/freetype.gpr";

library project OpenGL.FreeType is
   for Languages use ("ada");
   for Library_Name use "OpenGLAda_FreeType";
   for Source_Dirs use ("src/freetype");
   for Object_Dir use "obj";
   for Library_Dir use "lib";

   package Compiler renames OpenGL_Shared.Compiler;
end OpenGL.FreeType;

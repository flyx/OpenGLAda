limited with "../../../opengl-glfw.gpr";
limited with "../../../opengl-freetype.gpr";

project Store_A_Character is

   for Source_Dirs use ("src", "../../common");
   for Object_Dir use "obj/";
   for Main use ("store_character.adb");
   for Exec_Dir use ".";

   package Compiler is
      for Default_Switches ("ada") use ("-gnatyclux");
   end Compiler;

   package Linker is
      for Switches ("ada") use ("-L/usr/local/lib", "-lfreetype");
   end Linker;


end Store_A_Character;


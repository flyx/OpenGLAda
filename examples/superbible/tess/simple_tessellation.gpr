with  "../../common/common.gpr";
limited with "../../../opengl-glfw.gpr";

project Simple_Tessellation is

   for Source_Dirs use ("src");
   for Object_Dir use "obj/";
   for Exec_Dir use ".";
   for Main use ("simple_tessellation.adb");

   package Pretty_Printer is
      for Default_Switches ("ada") use ("-l3");
   end Pretty_Printer;

   package Compiler is
      for Default_Switches ("ada") use ("-gnatylctux", "-gnat12");
   end Compiler;

end Simple_Tessellation;
